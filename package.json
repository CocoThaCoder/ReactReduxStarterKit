{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:CocoBeezy/ReactStarterKit.git",
  "scripts": {
    "start": "node server.dev.js",
    "build": "npm run clean && webpack --config webpack.config.prod.js --progress --colors",
    "deploy": "pm2 start server.js",
    "clean": "rimraf dist",
    "test": "jest --colors",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Socorro Aguilar Jr.",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/src/SCSSStub.js"
    }
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^21.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-stage-1": "^6.1.18",
    "browser-sync": "^2.12.8",
    "del": "^2.2.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "history": "^2.0.1",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "material-ui": "^1.0.0-beta.19",
    "material-ui-icons": "^1.0.0-beta.17",
    "node-sass": "^3.13.0",
    "pm2": "^2.1.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "uglify": "^0.1.5",
    "webpack-stream": "^3.2.0"
  }
}
